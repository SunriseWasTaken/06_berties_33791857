<!DOCTYPE html>
<html>
<head>
    <title>User List</title>
</head>
<body>
    <h1>All Users</h1>

    <table border="1" cellpadding="8">
        <tr>
            <th>Username</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>

        <% users.forEach(user => { %>
            <tr>
                <td><%= user.username %></td>
                <td><%= user.first %></td>
                <td><%= user.last %></td>
                <td><%= user.email %></td>
            </tr>
        <% }) %>
    </table>
</body>
</html>